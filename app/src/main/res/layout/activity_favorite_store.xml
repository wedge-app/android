<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:theme="@style/ThemeOverlay.2021_Wedge.FullscreenContainer"
    tools:context=".MyPageFrag.gymrestaurant">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:gravity="center"
            android:text="@string/mhfg"
            android:textColor="#000000"
            android:textSize="30sp"
            android:fontFamily="@font/scdream5"
            android:textStyle="bold" />

        <Spinner
            android:id="@+id/spinner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="255dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/yellowbox"
            android:entries="@array/array_list"
            android:gravity="center"
            android:layout_marginStart="255dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="20dp"
            android:gravity="left"
            android:text="@string/hgr"
            android:textColor="#000000"
            android:textSize="24sp"
            android:fontFamily="@font/scdream4"
            android:layout_marginStart="50dp"
            tools:ignore="RtlHardcoded" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/greyline"
            tools:ignore="ContentDescription"
            android:layout_marginStart="40dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="20dp"
            android:gravity="left"
            android:text="@string/mfhxbg"
            android:textColor="#000000"
            android:textSize="24sp"
            android:layout_marginStart="50dp"
            android:fontFamily="@font/scdream4"
            tools:ignore="RtlHardcoded" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/greyline"
            tools:ignore="ContentDescription"
            android:layout_marginStart="40dp" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="17dp"
                android:layout_height="17dp"
                android:layout_marginLeft="267dp"
                android:layout_marginTop="103dp"
                android:background="#FFC812"
                app:srcCompat="@drawable/selectionline"
                tools:ignore="ContentDescription"
                android:layout_marginStart="267dp" />
        </LinearLayout>
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="20sp"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/back"
            android:background="#FFFFFF"
            android:contentDescription="@string/todo" />

    </LinearLayout>
</FrameLayout>