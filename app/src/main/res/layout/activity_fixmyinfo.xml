<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#ffffff"
    android:theme="@style/ThemeOverlay.2021_Wedge.FullscreenContainer"
    tools:context=".MyPageFrag.fixmyinfo">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
                    android:background="#FFFFFF"
                    android:gravity="left"
                    app:srcCompat="@drawable/back"
                    tools:ignore="ContentDescription,RtlHardcoded"
                    android:layout_marginStart="20dp" />

                <Button
                    android:id="@+id/button8"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:visibility="invisible"
                    tools:visibility="invisible" />
            </LinearLayout>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"
                android:autoSizeTextType="uniform"
                android:gravity="center"
                android:text="@string/fixmyinfo2"
                android:textColor="#000000"
                android:fontFamily="@font/scdream5"
                android:textSize="22sp"
                tools:targetApi="o" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="300dp"
                android:layout_marginBottom="10dp"
                app:srcCompat="@drawable/join_id"
                tools:ignore="ContentDescription"
                android:layout_marginEnd="300dp" />

            <EditText
                android:id="@+id/id"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/login_enter"
                android:ems="10"
                android:textSize="15sp"
                android:textColor="#8F9090"
                android:layout_marginTop="10dp"
                android:layout_marginRight="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginBottom="30dp"
                android:paddingHorizontal="30dp"
                android:hint="@string/bafsV"
                android:autoSizeTextType="uniform"
                android:inputType="textPersonName"
                android:fontFamily="@font/scdream4"
                android:autofillHints=""
                tools:targetApi="o" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="240dp"
                android:layout_marginBottom="10dp"
                app:srcCompat="@drawable/join_password"
                tools:ignore="ContentDescription"
                android:layout_marginEnd="240dp" />

            <EditText
                android:id="@+id/pwd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="30dp"
                android:layout_marginBottom="30dp"
                android:autoSizeTextType="uniform"
                android:background="@drawable/login_enter"
                android:ems="10"
                android:hint="@string/vff"
                android:inputType="textPassword"
                android:paddingHorizontal="30dp"
                android:textColor="#8F9090"
                android:textSize="15sp"
                android:fontFamily="@font/scdream4"
                android:autofillHints=""
                tools:targetApi="o" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="280dp"
                android:layout_marginBottom="10dp"
                app:srcCompat="@drawable/join_name"
                tools:ignore="ContentDescription"
                android:layout_marginEnd="280dp" />

            <EditText
                android:id="@+id/name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/login_enter"
                android:ems="10"
                android:textSize="15sp"
                android:textColor="#8F9090"
                android:layout_marginTop="10dp"
                android:layout_marginRight="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginBottom="30dp"
                android:paddingHorizontal="30dp"
                android:hint="@string/afsv"
                android:autoSizeTextType="uniform"
                android:inputType="textPersonName"
                android:fontFamily="@font/scdream4"
                android:autofillHints=""
                tools:targetApi="o" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="180dp"
                android:layout_marginBottom="10dp"
                app:srcCompat="@drawable/join_phone_number"
                tools:ignore="ContentDescription"
                android:layout_marginEnd="180dp" />

            <EditText
                android:id="@+id/tel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:textSize="15sp"
                android:textColor="#8F9090"
                android:layout_marginTop="10dp"
                android:layout_marginRight="30dp"
                android:layout_marginLeft="30dp"
                android:layout_marginBottom="30dp"
                android:paddingHorizontal="30dp"
                android:background="@drawable/login_enter"
                android:hint="@string/cxcfdsfaX"
                android:fontFamily="@font/scdream4"
                android:autoSizeTextType="uniform"
                android:inputType="phone"
                android:autofillHints=""
                tools:targetApi="o" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/fixsave"
                android:layout_width="100dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginBottom="10dp"
                android:background="@drawable/enter_box"
                android:text="수정 완료"
                android:fontFamily="@font/scdream4"
                android:textSize="20sp"
                android:textColor="@color/black" />

        </LinearLayout>
    </FrameLayout>



</FrameLayout>